
@model ContosoUniversity.Models.ConfigModel

@{
    ViewBag.Title = "Config";
}


<h2>Config</h2>

<ul>
    <li>
        <b>@Html.DisplayNameFor(model => model.OutputDirectory)</b>
        @Html.DisplayName(Model.OutputDirectory)
    </li>
    <li>
        <b>@Html.DisplayNameFor(model => model.SourceName)</b>
        @Html.DisplayName(Model.SourceName)
    </li>
    <li>
        <b>@Html.DisplayNameFor(model => model.LogName)</b>
        @Html.DisplayName(Model.LogName)
    </li>
    <li>
        <b>@Html.DisplayNameFor(model => model.ThumbnailSize)</b>
        @Html.DisplayName(Model.ThumbnailSize)
    </li>
	<li>
        <b>@Html.DisplayNameFor(model => model.Handlers)</b>
        <Table class="table">
            @foreach (var handler in Model.Handlers)
             {
         <tr>
            <td>
                <button>
                    @Html.ActionLink(@handler, "RemoveHandler", "Home", new{ handlerToRemove = handler}, null)
                </button>
            </td>
        </tr>
}
</li>
</ul>