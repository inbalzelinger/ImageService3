

@model WebProgect.Models.LogsWebModel

@{
    ViewBag.Title = "Logs";
}


<h2>Logs</h2>
<p>here you can write the types you can to only see</p>
<input type="text" id="filterField"><br>
<button id="filter">filter</button>


<table style="width:100%">
    <tr>
        <th>Status</th>
        <th>Messege</th>
    </tr>

    @foreach (var log in Model.LogsList)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => log.Item1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => log.Item2)
            </td>
        </tr>
    }
</table>
